@model List<Interext.Models.ChooseUsersViewModel>
@{
    Layout = null;
}
@Html.AntiForgeryToken()

@for (var i = 0; i < Model.Count; i++)
    {
    <tr>
        <td>
            @Html.CheckBoxFor(m => m[i].Checked)
        </td>
        <td>
            <img src="@Model[i].ImageUrl " class="small-user-pic"/>
        </td>
    </tr>
}